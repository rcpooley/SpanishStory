{
  "settings": {
    "roundtime": 15000
  },
  "strings": {
    "start-prompt": "Es el quinto día de tu primera semana de trabajo. Estás conduciendo a tu trabajo. No tienes mucho tiempo, pero tienes hambre.",
    "start-opt-comer": "Es necesario que <<comer>> algo. Vas a parar y comprar donas (doughnuts)",
    "start-opt-estar": "Es importante que no <<estar>> tarde. No vas a comer."
  },
  "rounds": {
    "start": {
      "prompt": "[start-prompt]",
      "opts": [
        {
          "text": "[start-opt-comer]",
          "next": "start-comer"
        },
        {
          "text": "[start-opt-estar]",
          "next": "start-estar"
        }
      ]
    },
    "start-comer": {
      "prompt": "[start-opt-comer]",
      "next": "boss-food",
      "opts": [
        "Comas",
        "Comes",
        "Comiendo",
        "Comería"
      ]
    },
    "start-estar": {
      "prompt": "[start-opt-estar]",
      "next": "boss-ontime",
      "opts": [
        "estés",
        "estás",
        "estar",
        "estando"
      ]
    },
    "boss-food": {
      "prompt": "food"
    },
    "boss-ontime": {
      "prompt": "ontime"
    },
    "error": {
      "prompt": "An error has occurred"
    }
  }
}